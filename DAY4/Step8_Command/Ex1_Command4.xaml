<Window x:Class="Step8_Command.Ex1_Command4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Step8_Command"
        mc:Ignorable="d"
        Title="Ex1_Command4" Height="450" Width="800">
    <StackPanel>

        <TextBox x:Name="id_txtbox" Width="200" TextChanged="id_txtbox_TextChanged"/>

        <CheckBox x:Name="check" Content="동의" Width="200" Unchecked="check_Unchecked" Checked="check_Checked"/>

        <Button IsEnabled="True" x:Name="loginbutton" Content="login" Width="200"
                Command="local:MyCommand3.cmdLogin"/>


    </StackPanel>
</Window>
